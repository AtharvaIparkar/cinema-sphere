{
    "version": 2,
    "buildCommand": "cd client-vite && npm install && npm run build",
    "outputDirectory": "client-vite/dist",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}